<div class="container">
    <div class="column">
        <p class="has-text-white title is-large"><i class="fa fa-ad has-text-info"></i>&nbsp;Premium Botlar</p>
        <p class="content has-text-white" style="margin-top: -20px;">Aldıkları oylara göre sıralanmaktadırlar.</p>
      </div>
      <div class="columns is-multiline is-desktop is-variable is-7 pt-5">
        <% botdata.sort((a, b) => b.votes - a.votes).slice(0, 8).filter(a => a.premium == "Premium").map(a => {%>
                <div class="column is-3 mb-5">    
                    <div style="background-color: #23272a;" class="box cardvcodesbotCard">
                      <div>
                        <img draggable="false" onerror="imgError(this);" alt="<%= a.username %>#<%=a.discrim%>" src="<%= a.avatar %>" class="is-pulled-left cardvcodesbotIMG z">
                      </div>
                      <div class="is-pulled-right is-inline-block">
                         <span class="tag is-dark mt-1 w-100">Votes: <%= a.votes || "0" %></span>
                         </div>
                       <br><br><br>
                      <p style="margin-top: 5px;" class="cardvcodesbotName has-text-white mt-3">
                     <%= a.username %>
                     <% if(a.certificate == "Certified") {%>
                        <a data-tooltip="Doğrulanmış Bot"
                        data-tooltip-location="top"><i class="fad fa-badge-check has-text-primary"></i></a>
                     <% } %>
                     <% if(a.premium == "Premium") {%>
                        <a data-tooltip="Premium Bot"
                        data-tooltip-location="top"><i class="fad fa-diamond has-text-primary"></i></a>
                     <% } %>
                      <p style="margin-top: -10px;" class="cardvcodesbotDesc"><%= a.shortDesc %></p>
                      <center>
    
                      <div class="cardvcodesbotButtons">
                          <a href="/bot/<%= a.botID %>" title="CodeList" class="btn btn-card"><i class="fad fa-eye"></i>&nbsp;Görüntüle</a>
                          &nbsp;&nbsp;<a target="_blank" title="CodeList" href="/redirect?type=botInvite&botID=<%= a.botID %>" class="btn btn-card"><i class="fad fa-user-plus"></i>&nbsp;Davet</a>
                          <br><br>
    
                         </div>
                     </center>
                    </div>
                </div>
            <% }) %>
    
             </div>
             <br><br>
    <div class="column">
    <p class="has-text-white title is-large"><i class="fa fa-badge-check has-text-warning"></i>&nbsp;Sertifikalı Botlar</p>
    <p class="content has-text-white" style="margin-top: -20px;">Aldıkları oylara göre sıralanmaktadırlar.</p>
    <div class="columns is-multiline is-desktop is-variable is-7 pt-5">
        <% botdata.sort((a, b) => b.votes - a.votes).slice(0, 8).filter(a => a.certificate == "Certified").map(a => {%>
                <div class="column is-3 mb-5">    
                    <div style="background-color: #23272a;" class="box cardvcodesbotCard">
                      <div>
                        <img draggable="false" onerror="imgError(this);" alt="<%= a.username %>#<%=a.discrim%>" src="<%= a.avatar %>" class="is-pulled-left cardvcodesbotIMG z">
                      </div>
                      <div class="is-pulled-right is-inline-block">
                            <span class="tag is-dark mt-1 w-100">Votes: <%= a.votes || "0" %></span>
                         </div>
                       <br><br><br>
                      <p style="margin-top: 5px;" class="cardvcodesbotName has-text-white mt-3">
                        <%= a.username %>
                     <% if(a.certificate == "Certified") {%>
                        <a data-tooltip="Certified Bot"
                        data-tooltip-location="top"><i class="fad fa-badge-check has-text-primary"></i></a>
                     <% } %>
                     <% if(a.premium == "Premium") {%>
                        <a data-tooltip="Premium Bot"
                        data-tooltip-location="top"><i class="fad fa-diamond has-text-primary"></i></a>
                     <% } %>
                      <p style="margin-top: -10px;" class="cardvcodesbotDesc"><%= a.shortDesc %></p>
                      <center>
    
                      <div class="cardvcodesbotButtons">
                          <a href="/bot/<%= a.botID %>" title="vCodes" class="btn btn-card"><i class="fad fa-eye"></i>&nbsp;View</a>
                          &nbsp;&nbsp;<a target="_blank" title="vCodes" href="/redirect?type=botInvite&botID=<%= a.botID %>" class="btn btn-card"><i class="fad fa-user-plus"></i>&nbsp;Invite</a>
                          <br><br>
    
                         </div>
                     </center>
                    </div>
                </div>
            <% }) %>
    
             </div>
</div>
<br><br>
<div class="column">
    <p class="has-text-white title is-large"><i class="fad fa-fire has-text-danger"></i>&nbsp;Popüler Botlar</p>
    <p class="content has-text-white" style="margin-top: -20px;">Aldıkları oylara göre sıralanmaktadırlar.</p>
  </div>
  <div class="columns is-multiline is-desktop is-variable is-7 pt-5">
    <% botdata.sort((a, b) => b.votes - a.votes).slice(0, 6).filter(a => a.status == "Approved").map(a => {%>
            <div class="column is-3 mb-5">    
                <div style="background-color: #23272a;" class="box cardvcodesbotCard">
                  <div>
                    <img draggable="false" onerror="imgError(this);" alt="<%= a.username %>#<%=a.discrim%>" src="<%= a.avatar %>" class="is-pulled-left cardvcodesbotIMG z">
                  </div>
                  <div class="is-pulled-right is-inline-block">
                        <span class="tag is-dark mt-1 w-100">Votes: <%= a.votes || "0" %></span>
                     </div>
                   <br><br><br>
                  <p style="margin-top: 5px;" class="cardvcodesbotName has-text-white mt-3">
                    <%= a.username %>
                 <% if(a.certificate == "Certified") {%>
                    <a data-tooltip="Verified Bot"
                    data-tooltip-location="top"><i class="fad fa-badge-check has-text-primary"></i></a>
                 <% } %>
                 <% if(a.premium == "Premium") {%>
                    <a data-tooltip="Premium Bot"
                    data-tooltip-location="top"><i class="fad fa-diamond has-text-primary"></i></a>
                 <% } %>
                 </p>   
                  <p style="margin-top: -10px;" class="cardvcodesbotDesc"><%= a.shortDesc %></p>
                  <center>
                  <div class="cardvcodesbotButtons">
                      <a href="/bot/<%= a.botID %>" title="vCodes" class="btn btn-card"><i class="fad fa-eye"></i>&nbsp;View</a>
                      &nbsp;&nbsp;<a target="_blank" title="vCodes" href="/redirect?type=botInvite&botID=<%= a.botID %>" class="btn btn-card"><i class="fad fa-user-plus"></i>&nbsp;Invite</a>
                      <br><br>
                     </div>
                 </center>
                </div>
            </div>
        <% }) %>

         </div>
</div>
<br>
    <center>
    <button onclick="location.href='/bots'" class="btn btn-back"><i class="fad fa-check"></i>&nbsp; Bütün Botları Görüntüle.</button>
    </center>
<%- include('parts/foot') %>
