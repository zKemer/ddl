<%- include('../../parts/head', {bot, user, path}) %>
<section class="bg-vcodes jumbotron">
    <div class="container px-3 mt-1 mb-5">
        <div class="mb-3 mx-auto has-text-centered">
          <img draggable="false" style="z-index: -1!important" src="https://cdn.discordapp.com/attachments/831418070785982466/831444449706901504/ezgif.com-gif-maker-removebg-preview_4.png" width="220" alt="Discord Bots | Discord Bot List">
          <h1 class="has-text-white title" style="margin-top: -2.3%; font-weight: 800;">CodeList</h1>
          <p class="content has-text-white" style="opacity: 0.6; margin-top: -20px;">
            Botunuzu geliştirmek için kodları alabilirsiniz.</p>
        </div>
<br><br><br><br><br>
      </div>
  </div>
</section>
    <svg style="margin-top: -9%; pointer-events: none" class="wave" width="100%" height="100px" preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 1920 75">
        <defs>
            <style>
                .ae {
                    fill: none;
                }
                
                .b {
                    clip-path: url(#a);
                }
                
                .c,
                .d {
                    fill: #2c2f33;
                }
                
                .d {
                    opacity: 0.5;
                    isolation: isolate;
                }
            </style>
            <clipPath id="a">
                <rect class="a" width="1920" height="75"></rect>
            </clipPath>
        </defs>
        <title>wave</title>
        <g class="b">
            <path class="c" d="M1963,327H-105V65A2647.49,2647.49,0,0,1,431,19c217.7,3.5,239.6,30.8,470,36,297.3,6.7,367.5-36.2,642-28a2511.41,2511.41,0,0,1,420,48"></path>
        </g>
        <g class="b">
            <path class="d" d="M-127,404H1963V44c-140.1-28-343.3-46.7-566,22-75.5,23.3-118.5,45.9-162,64-48.6,20.2-404.7,128-784,0C355.2,97.7,341.6,78.3,235,50,86.6,10.6-41.8,6.9-127,10"></path>
        </g>
        <g class="b">
            <path class="d" d="M1979,462-155,446V106C251.8,20.2,576.6,15.9,805,30c167.4,10.3,322.3,32.9,680,56,207,13.4,378,20.3,494,24"></path>
        </g>
        <g class="b">
            <path class="d" d="M1998,484H-243V100c445.8,26.8,794.2-4.1,1035-39,141-20.4,231.1-40.1,378-45,349.6-11.6,636.7,73.8,828,150"></path>
        </g>
        <br><br><br>
    </svg>

    <div class="container">
        <div class="column">
            <p class="has-text-white title is-large"><i class="fa fa-code has-text-info"></i>&nbsp; <%= req.params.type.toUpperCase() %> Codes</p>
          </div>
          <table id="tableData" class="table table-dark table-striped" data-toggle="table" data-pagination="true" data-search="true">
            <thead>
            <th>Kod İsmi</th>
            <th>Kod Açıklama</th>
            <th>İşlem</th>
            </thead>
                    <% data.filter(a => a.codeCategory.toLowerCase() === req.params.type.toLowerCase()).map(a => {%>
                      <tr>
                          <td><%= a.codeName %></td>
                          <td><%= a.codeDesc %></td>
                        <td><a href="/code/<%= a.code %>" style="padding: 5px 25px;" class="btn btn-vote has-text-white">View</a></td>
                      </tr>
                   <% }); %>
          </table>
    </div>
</center>
<%- include('../../parts/foot') %>
